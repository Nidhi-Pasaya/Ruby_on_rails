<div class="homepage p-5" >
  <div class="card shadow rounded" style="height: auto; width: 305px; background-color: transparent;">
  <%= flash[:notice]%>
    <div class="card-body"  style="padding: 35px;">
      <div class=" text-center">
        <h4>Log In</h4>
      </div>
      <%= form_with do |f| %>
        <br/>
        <div class="field mb-3">
          <%= f.text_field :username, autofocus: true, class:"form-control", placeholder:"Username" %>
        </div>

        <div class="field mb-3">
          <%= f.password_field :password, class:"form-control", placeholder:"Password" %>
        </div>
        <br/>
        <div class="actions text-center">
          <%= f.submit "Log in", class:"form-control btn btn-secondary" %>
        </div>
      <% end %>
      <br/>
    </div>
  </div>
</div>
















